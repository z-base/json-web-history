{
  "name": "jwh",
  "version": "0.0.0",
  "description": "",
  "keywords": [
    "typescript",
    "z-base"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z-base/jwh.git"
  },
  "bugs": {
    "url": "https://github.com/z-base/jwh/issues"
  },
  "homepage": "z-base.github.io/jwh/",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "clean:dist": "node -e \"require('node:fs').rmSync('dist',{recursive:true,force:true})\"",
    "format": "prettier . --write",
    "build": "npm run clean:dist && tsc -p tsconfig.json",
    "test": "npm run build && c8 --reporter=text --reporter=text-summary --check-coverage --lines 100 --functions 100 --branches 100 --statements 100 node test/run-all.mjs",
    "bench": "npm run build && node benchmark/bench.js",
    "prepare": "husky",
    "prepublishOnly": "npm run test && npm run bench",
    "deploy": "wrangler deploy",
    "dev": "wrangler dev",
    "start": "wrangler dev"
  },
  "c8": {
    "all": true,
    "include": [
      "dist/**/*.js"
    ],
    "exclude": [
      "dist/**/*.d.ts",
      "test/**",
      "benchmark/**",
      "playwright.config.ts"
    ]
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@playwright/test": "^1.58.2",
    "@types/node": "^25.3.0",
    "c8": "^10.1.3",
    "fast-glob": "^3.3.3",
    "husky": "^9.1.7",
    "playwright": "^1.58.2",
    "prettier": "^3.8.1",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.1.3",
    "@z-base/bytecodec": "^1.1.0",
    "@z-base/cryptosuite": "^1.0.1"
  }
}
